# API Keys (get these from your exchange accounts)
NEXT_PUBLIC_COINBASE_API_KEY=your_coinbase_api_key_here
NEXT_PUBLIC_COINBASE_API_SECRET=your_coinbase_api_secret_here
NEXT_PUBLIC_KRAKEN_API_KEY=your_kraken_api_key_here
NEXT_PUBLIC_KRAKEN_API_SECRET=your_kraken_api_secret_here

# App Configuration
NEXT_PUBLIC_DEFAULT_EXCHANGE=coinbase
NEXT_PUBLIC_DEFAULT_PAIR=BTC-USD
NEXT_PUBLIC_DEFAULT_TIMEFRAME=1h

# Development Settings
NEXT_PUBLIC_DEBUG=false

# API Routes (usually don't need to change these)
NEXT_PUBLIC_API_BASE_URL=/api
NEXT_PUBLIC_WS_URL=wss://ws-feed.pro.coinbase.com
